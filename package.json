{
  "name": "plannitt",
  "version": "0.1.0",
  "description": "A collaborative timetable creator built for teams",
  "scripts": {
    "client": "yarn --cwd client run start",
    "server": "yarn --cwd server run start",
    "build": "yarn --cwd client run build",
    "start": "yarn server",
    "dev": "concurrently \"yarn run client\" \"yarn run server\""
  },
