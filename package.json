    "express": "^4.17.1",
    "http-proxy-middleware": "^2.0.1",
    "mongodb": "^4.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-modern-calendar-datepicker": "^3.1.6",
    "react-router-dom": "^5.2.1",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "dotenv": "^10.0.0"
  },
